[color]
  ui = true
[color "diff"]
  commit = yellow bold
  frag = magenta bold
  meta = yellow
  new = green bold
  old = red bold
  whitespace = red reverse
[color "diff-highlight"]
  newHighlight = green bold 22
  newNormal = green bold
  oldHighlight = red bold 52
  oldNormal = red bold
[commit]
  template = /Users/eubenesa/.stCommitMsg
[core]
  editor = code -w
  excludesfile = /Users/eubenesa/.gitignore_global
  pager = diff-so-fancy | less --tabs=2 -RFX
[diff-so-fancy]
  first-run = false
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =
[mergetool "sourcetree"]
  cmd = /Applications/Sourcetree-Beta.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[user]
  email = eubene.sa@gmail.com
  name = Eubene Sa
